                    <ImageButton
                        android:id="@+id/ba_t"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:padding="10dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/ba_t" />

                    <ImageButton
                        android:id="@+id/bi_t"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:padding="10dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/bi_t" />

                    <ImageButton
                        android:id="@+id/bu_t"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:padding="10dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/bu_t" />

                    <ImageButton
                        android:id="@+id/be_t"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:padding="10dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/be_t" />

                    <ImageButton
                        android:id="@+id/bo_t"
                        android:layout_width="50dp"
