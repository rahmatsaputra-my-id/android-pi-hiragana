                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:padding="10dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/ga_t" />

                    <ImageButton
                        android:id="@+id/gi_t"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_weight="1"
